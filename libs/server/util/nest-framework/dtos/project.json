{
	"name": "server-util-nest-framework-dtos",
	"$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/server/util/nest-framework/dtos/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/libs/server/util/nest-framework/dtos",
				"tsConfig": "libs/server/util/nest-framework/dtos/tsconfig.lib.json",
				"packageJson": "libs/server/util/nest-framework/dtos/package.json",
				"main": "libs/server/util/nest-framework/dtos/src/index.ts",
				"assets": ["libs/server/util/nest-framework/dtos/*.md"]
			}
		},
		"lint": {
			"executor": "@nx/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["libs/server/util/nest-framework/dtos/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/server/util/nest-framework/dtos/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		}
	},
	"tags": ["scope:backend", "scope:lib"]
}
